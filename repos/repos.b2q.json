[  {
    "kernelSource": {
      "name": "kernel_samsung_b2q",
      "repo": "https://github.com/morosakubek2/android_kernel_samsung_b2q",
      "branch": "main",
      "device": "b2q_eur_openx"
    },
    "withKernelSU": true,
     "toolchains": [
      {
        "repo": "https://android.googlesource.com/platform/prebuilts/gas/linux-x86",
        "branch": "master",
        "name": "gas"
      },
      {
        "repo": "https://gitlab.com/mizdrake7/zyc_clang.git",
        "branch": "master",
        "name": "clang"
      }
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommand": {
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "arm-linux-androideabi-",
        "LD": "ld.lld",
        "AR": "llvm-ar",
        "NM": "llvm-nm",
        "OBJCOPY": "llvm-objcopy",
        "OBJDUMP": "llvm-objdump",
        "READELF": "llvm-readelf",
        "OBJSIZE": "llvm-size",
        "STRIP": "llvm-strip",
        "LDGOLD": "aarch64-linux-gnu-ld.gold",
        "LLVM_AR": "llvm-ar",
        "LLVM_DIS": "llvm-dis",
        "CONFIG_THINLTO": ""
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "repo": "https://github.com/morosakubek2/AnyKernel3",
      "branch": "b2q"
    }
  }
]
