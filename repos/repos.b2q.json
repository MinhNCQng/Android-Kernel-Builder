[  {
    "kernelSource": {
      "name": "kernel_samsung_b2q",
      "repo": "https://github.com/morosakubek2/android_kernel_samsung_b2q",
      "branch": "main",
      "device": "b2q_eur_openx",
      "defconfig": "b2q_eur_openx_defconfig"
    },
    "withKernelSU": true,
    "toolchains": [
  {
    "repo": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/arm/arm-linux-androideabi-4.9/",
    "branch": "android-10.0.0_r32",
    "name": "gcc",
    "binPath": ["bin"]
  },
  {
    "repo": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9/",
    "branch": "android-10.0.0_r32",
    "name": "gcc64",
    "binPath": ["bin"]
  },
  {
    "url": "https://github.com/ZyCromerZ/Clang/releases/download/19.0.0git-20240428-release/Clang-19.0.0git-20240428.tar.gz",
    "name": "clang",
    "binPath": ["./bin"]
  } 
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommand": {
        "CROSS_COMPILE": "gcc64/aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "gcc/arm-linux-androideabi-",
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "LLVM_IAS": "1",
        "LLVM": "1"
      }
    }
    "AnyKernel3": {
      "use": true,
      "release": true,
      "repo": "https://github.com/morosakubek2/AnyKernel3",
      "branch": "b2q"
    }
  }
]
