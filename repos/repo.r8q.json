[
  {
    "kernelSource": {
      "name": "r8q_Samsung_KenrlSU",
      "repo": "https://github.com/anuragpindoriya/android_kernel_samsung_sm8250",
      "branch": "prime",
      "device": "r8q",
      "defconfig": "r8q_defconfig"
    },
    "withKernelSU": true,
    "toolchains": [
      {
        "url": "https://github.com/ZyCromerZ/Clang/releases/download/18.0.0-20230915-release/Clang-18.0.0-20230915.tar.gz",
        "name": "clang",
        "binPath": ["./bin"]
      }
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang/bin/clang",
      "externalCommand": {
        "CLANG_TRIPLE": "clang/bin/aarch64-linux-android-",
        "CROSS_COMPILE": "clang/bin/aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "clang/bin/arm-linux-androideabi-",
        "AR": "clang/bin/llvm-ar",
        "NM": "clang/bin/llvm-nm",
        "OBJCOPY": "clang/bin/llvm-objcopy",
        "OBJDUMP": "clang/bin/llvm-objdump",
        "READELF": "clang/bin/llvm-readelf",
        "OBJSIZE": "clang/bin/llvm-size",
        "STRIP": "clang/bin/llvm-strip",
        "LOCALVERSION": "An-Unknown-r8q"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": false,
      "repo": "https://github.com/easterNday/AnyKernel3/",
      "branch": "thyme"
    }
  }
]
